{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "path": "coaching_mcp/server.py",
    "entrypoint": "mcp"
  },
  "environment": {
    "type": "uv",
    "project": ".",
    "editable": ["."]
  },
  "deployment": {
    "transport": "sse",
    "env": {
      "GONG_API_KEY": "${GONG_API_KEY}",
      "GONG_API_SECRET": "${GONG_API_SECRET}",
      "GONG_API_BASE_URL": "${GONG_API_BASE_URL}",
      "ANTHROPIC_API_KEY": "${ANTHROPIC_API_KEY}",
      "DATABASE_URL": "${DATABASE_URL}",
      "GONG_WEBHOOK_SECRET": "${GONG_WEBHOOK_SECRET}",
      "LOG_LEVEL": "${LOG_LEVEL}"
    }
  }
}
